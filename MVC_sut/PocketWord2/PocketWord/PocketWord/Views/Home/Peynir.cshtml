    <style>

        html, body {
            max-width: 100%;
            overflow-x: hidden;
        }

        h2 {
            text-align: center;
        }

        .container {
            display: flex;
            flex-wrap: wrap;
            justify-content: space-between;
            min-width: 1500px;
        }

        .box {
            width: calc(25% - 10px);
            height: 580px;
            background-color: #f0f0f0;
            border: 3px solid #000;
            margin-bottom: 10px;
            padding: 10px;
            border-radius: 10px;
            display: flex;
            flex-direction: column;
        }

        .resim {
            height: 470px;
            width: 100%; /* Resimlerin container'ı doldurması için */
            border: 2px solid #000;
            border-radius: 10px;
            object-fit: cover; /* Resimlerin oranlarını koruyarak sığması için */
        }

        .btn {
            width: 300px;
            z-index: 1000;
            position: relative;
            left: -1170px;
            top: 29px;
            margin-top: 5px;
        }

        .yazi {
            text-align: center;
            font-weight: bolder;
            font-size: 20px;
            color: black;
            text-align: center;
        }

        .box .container {
            margin-top: auto;
        }


        * {
            box-sizing: border-box;
        }

        .foto {
            max-width: 400px;
            height: 600px;
            margin: auto; /* Resmi yatayda ortalamak için */
            display: block; /* Resmi blok olarak göstermek için */
        }

        .carousel-control-prev,
        .carousel-control-next {
            background-color: rgba(0, 0, 0, 0.5);
            border-radius: 50%;
            z-index: 10;
            opacity: 1;
            height: 50px;
            width: 50px;
            position: absolute;
            top: 250px; /* Butonları 200px aşağıya indirir */
        }


        .carousel-control-prev-icon {
            color: white;
            font-size: 24px;
            line-height: 50px;
            content: "\2039"; /* Sol ok karakteri */
        }

        .carousel-control-next-icon {
            color: white;
            font-size: 24px;
            line-height: 50px;
            content: "\203A"; /* Sağ ok karakteri */
